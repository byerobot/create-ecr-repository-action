{
  "name": "create-ecr-repository-action",
  "version": "0.0.0",
  "private": true,
  "description": "Create an ECR repository if it doesn't exist",
  "main": "lib/src/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint **/*.ts",
    "package": "ncc build --source-map --license licenses.txt",
    "test": "jest",
    "check": "yarn format && yarn lint && yarn test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/int128/create-ecr-repository-action.git"
  },
  "keywords": [
    "actions",
    "aws"
  ],
  "author": "byerobot",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.10",
    "@aws-sdk/client-ecr": "3.511.0"
  },
  "devDependencies": {
    "@tsconfig/node20": "^20.1.2",
    "@types/jest": "29.5.12",
    "@types/node": "20.11.17",
    "@typescript-eslint/eslint-plugin": "7.0.1",
    "@typescript-eslint/parser": "7.0.1",
    "@vercel/ncc": "0.38.1",
    "aws-sdk-client-mock": "3.0.1",
    "eslint": "8.56.0",
    "eslint-plugin-jest": "27.6.3",
    "jest": "29.7.0",
    "js-yaml": "4.1.0",
    "prettier": "3.2.5",
    "ts-jest": "29.1.2",
    "typescript": "5.3.3"
  }
}
